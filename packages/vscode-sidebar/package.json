{
  "name": "alova-vscode-sidebar",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "watch": "vite build --watch",
    "build:app": "vite build",
    "build:copy": "ncp ./dist ../vscode-extension/sidebar-out",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@jsonrpc-rx/client": "catalog:",
    "@vueuse/core": "catalog:frontend",
    "path-browserify": "catalog:",
    "pinia": "catalog:frontend",
    "vue": "catalog:frontend",
    "vue-i18n": "catalog:frontend",
    "vue-router": "catalog:frontend"
  },
  "devDependencies": {
    "@types/path-browserify": "catalog:",
    "@vitejs/plugin-vue": "catalog:build",
    "@vue/test-utils": "catalog:dev",
    "alova-vscode-extension": "workspace:",
    "jsdom": "catalog:",
    "vite": "catalog:build",
    "vite-plugin-vscode-webview-hmr": "catalog:build",
    "vite-plugin-vue-devtools": "catalog:build",
    "vitest": "catalog:dev",
    "vue-tsc": "catalog:dev"
  }
}
