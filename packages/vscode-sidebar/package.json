{
  "name": "alova-vscode-sidebar",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite-ssg build",
    "build:sidebar": "vite build --outDir ../vscode-extension/out/sidebar",
    "watch": "vite build --watch",
    "watch:sidebar": "vite build --watch --outDir ../vscode-extension/out/sidebar",
    "dev": "vite --port 3333 --open",
    "vscode:dev": "",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "sizecheck": "npx vite-bundle-visualizer"
  },
